  # Pods for __PRODUCT_NAME__
  # NOTE: This must match the dependencies listed in __PRODUCT_NAME__.podspec.

  # We define the dependencies in the Podspec so they are pulled in when
  # integrating __PRODUCT_NAME__ via CocoaPods.
  # We define the dependencies here so they are pulled in to the project/workspace
  # during development.

  target '__PRODUCT_NAME__Tests' do
    inherit! :search_paths
    # Pods for testing
