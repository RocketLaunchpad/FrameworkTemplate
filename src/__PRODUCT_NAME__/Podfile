end

target '__PRODUCT_NAME__Example' do
  use_frameworks!

  # Pods for __PRODUCT_NAME__Example

  # Because __PRODUCT_NAME__ uses CocoaPods to resolve its dependencies, we
  # need to include it via CocoaPods even though it lives in the same
  # project/workspace as the example project.
  pod '__PRODUCT_NAME__', :path => '.'
end

